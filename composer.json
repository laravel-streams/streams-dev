{
    "name": "streams/cli",
    "description": "CLI Tooling for Laravel Streams.",
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Streams\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Cli\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Streams\\Cli\\CliServiceProvider"
            ]
        }
    }
}
