{
    "name": "streams/dev",
    "description": "Laravel Streams development tooling.",
    "authors": [
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Streams\\Dev\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Streams\\Dev\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Streams\\Dev\\DevServiceProvider"
            ]
        }
    }
}
